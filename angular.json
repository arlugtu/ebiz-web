{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "product-bot-admin": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/product-bot-admin",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/assets/bootstrap/css/bootstrap.min.css",
                            "src/assets/css/dark/custom.css",
                            "src/assets/css/dark/main.css",
                            "src/assets/css/dark/scrollspyNav.css",
                            "src/assets/scss/dark/assets/custom.scss",
                            "src/assets/scss/dark/assets/main.scss",
                            "src/assets/scss/dark/assets/scrollspyNav.scss",
                            "src/assets/scss/dark/base/_base.scss",
                            "src/assets/scss/dark/base/_functions.scss",
                            "src/assets/scss/dark/base/_mixins.scss",
                            "src/assets/scss/dark/base/_utilities.scss",
                            "src/assets/scss/layouts/vertical-dark-menu/dark/loader.scss",
                            "src/assets/scss/layouts/vertical-dark-menu/dark/structure-mod.scss",
                            "src/assets/scss/layouts/vertical-dark-menu/dark/structure.scss",
                            "src/assets/css/dark/dashboard/dash_1.css",
                            "src/assets/layouts/vertical-dark-menu/css/dark/plugins.css",
                            "src/assets/css/dark/widgets/modules-widgets-vlm.css",
                            "src/assets/plugins/src/table/datatable/datatables.css",
                            "src/assets/plugins/css/dark/table/datatable/dt-global_style.css",
                            "src/assets/plugins/src/font-icons/fontawesome/css/regular.css",
                            "src/assets/plugins/src/font-icons/fontawesome/css/fontawesome.css",
                            "src/assets/css/dark/authentication/auth-boxed.css",
                            "src/assets/plugins/css/dark/stepper/custom-bsStepper.css",
                            "node_modules/bs-stepper/dist/css/bs-stepper.min.css",
                            "src/assets/plugins/src/noUiSlider/nouislider.min.css",
                            "src/assets/plugins/css/dark/noUiSlider/custom-nouiSlider.css",
                            "src/assets/plugins/css/dark/bootstrap-range-Slider/bootstrap-slider.css",
                            "src/assets/plugins/src/filepond/filepond.min.css",
                            "src/assets/plugins/src/filepond/FilePondPluginImagePreview.min.css",
                            "src/assets/plugins/css/dark/filepond/custom-filepond.css",
                            "node_modules/filepond/dist/filepond.min.css",
                            "node_modules/flatpickr/dist/flatpickr.min.css",
                            "src/assets/plugins/src/apex/apexcharts.css",
                            "node_modules/filepond-plugin-image-preview/dist/filepond-plugin-image-preview.css",
                            "src/assets/css/dark/users/account-setting.css",
                            "src/assets/css/dark/components/list-group.css",
                            "src/assets/css/dark/users/user-profile.css",
                            "src/assets/css/dark/pages/knowledge_base.css",
                            "node_modules/sweetalert2/src/sweetalert2.scss",
                            "node_modules/slick-carousel/slick/slick.scss",
                            "node_modules/slick-carousel/slick/slick-theme.scss",
                            "src/assets/css/dark/components/accordions.css",
                            "src/styles.scss"
                        ],
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/popper.js/dist/popper.js",
                            "src/assets/bootstrap/js/bootstrap.bundle.min.js",
                            "src/assets/plugins/src/perfect-scrollbar/perfect-scrollbar.min.js",
                            "src/assets/plugins/src/mousetrap/mousetrap.min.js",
                            "src/assets/plugins/src/waves/waves.min.js",
                            "src/assets/plugins/src/global/vendors.min.js",
                            "src/assets/js/custom.js",
                            "src/assets/plugins/src/table/datatable/datatables.js",
                            "src/assets/plugins/src/font-icons/feather/feather.min.js",
                            "src/assets/js/scrollspyNav.js",
                            "src/assets/plugins/src/noUiSlider/nouislider.min.js",
                            "src/assets/plugins/src/noUiSlider/wNumb.min.js",
                            "src/assets/plugins/src/noUiSlider/custom-nouiSlider.js",
                            "src/assets/plugins/src/bootstrap-range-Slider/bootstrap-rangeSlider.js",
                            "src/assets/plugins/src/filepond/filepond.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginFileValidateType.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginImageExifOrientation.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginImagePreview.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginImageCrop.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginImageResize.min.js",
                            "src/assets/plugins/src/filepond/FilePondPluginImageTransform.min.js",
                            "src/assets/plugins/src/filepond/filepondPluginFileValidateSize.min.js",
                            "src/assets/plugins/src/filepond/custom-filepond.js",
                            "src/assets/plugins/src/apex/apexcharts.min.js",
                            "src/assets/js/dashboard/dash_1.js",
                            "src/assets/js/users/account-settings.js",
                            "node_modules/slick-carousel/slick/slick.min.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "1mb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "product-bot-admin:build:production"
                        },
                        "development": {
                            "browserTarget": "product-bot-admin:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "product-bot-admin:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}
